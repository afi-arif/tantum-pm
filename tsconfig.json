{
    "compilerOptions": {
        "target": "ES5",
        "sourceMap": true,
        "lib": [
            "dom","DOM.Iterable","esnext"
        ],
        "allowJs": true,
        "skipLibCheck": true,
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "strict": true,
        "forceConsistentCasingInFileNames": false,
        "module": "esnext",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "jsx": "react",
        "removeComments": true       
    },
    "include": ["src"]
    
}